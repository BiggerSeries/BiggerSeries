{
  "multipart": [
    {
      "apply": {
        "model": "biggerreactors:block/reactor_power_port_disassembled"
      }
    },
    {
      "when": {
        "direction": "face",
        "connectionstate": "disconnected"
      },
      "apply": {
        "model": "biggerreactors:block/reactor_power_port_disconnected"
      }
    },
    {
      "when": {
        "direction": "face",
        "connectionstate": "connected"
      },
      "apply": {
        "model": "biggerreactors:block/reactor_power_port_connected"
      }
    }
  ]
}